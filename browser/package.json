{
  "name": "sendme-browser",
  "version": "0.1.0",
  "description": "Sendme file transfer in the browser via WebAssembly",
  "scripts": {
    "build": "cargo build --target=wasm32-unknown-unknown && wasm-bindgen ../target/wasm32-unknown-unknown/debug/sendme_browser.wasm --out-dir=public/wasm --weak-refs --target=web --debug",
    "build:release": "cargo build --target=wasm32-unknown-unknown --release && wasm-bindgen ../target/wasm32-unknown-unknown/release/sendme_browser.wasm --out-dir=public/wasm --weak-refs --target=web",
    "serve": "servor public"
  },
  "author": "n0 team",
  "license": "Apache-2.0 OR MIT",
  "dependencies": {
    "servor": "^4.0.2"
  }
}
